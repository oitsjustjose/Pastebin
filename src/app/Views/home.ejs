                    <option value="xojo">Xojo (REALbasic)</option>
                    <option value="yaml">YAML</option>
                </select>
            </div>
            <div class="submit-wrapper">
                <div class="mb-3" style="width: max-content;">
                    <label for="expiry">Expiration</label>
                    <select class="form-select" name="expiry" id="expiry">
                        <option value="60000" selected>1 Hour</option>
                        <option value="360000">6 Hours</option>
                        <option value="1440000">1 Day</option>
                        <option value="10080000">7 Days</option>
                        <option value="43200000">30 Days</option>
                        <option value="432000000000">Never</option>
                    </select>
                </div>
                <button type="submit" class="btn btn-primary">Upload</button>
            </div>
        </form>
    </div>
</body>

</html>